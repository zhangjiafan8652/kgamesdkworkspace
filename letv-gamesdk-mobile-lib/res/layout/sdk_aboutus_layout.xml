<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    >

    <com.le.legamesdk.widget.LeMainViewParent
        android:id="@+id/le_main_parent"
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        android:background="@drawable/le_border_line_bg">
    <LinearLayout
        android:id="@+id/sdk_aboutus_rl"
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        android:background="@drawable/le_border_line_bg_round_ten_dp"
        android:orientation="vertical">
        <include
            android:id="@+id/le_main_view_action_bar"
            layout="@layout/le_main_view_action_bar_layout"
            android:layout_width="match_parent"
            android:layout_height="@dimen/le_main_view_action_bar_height"
            />
        <ImageView
            android:id="@+id/sdk_aboutus_lelogo_iv"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:src="@drawable/pic_aboutus"
            android:layout_gravity="center"/>
        <TextView
            android:id="@+id/sdk_aboutus_lelogo_tv"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:gravity="center"
            android:layout_marginTop="13dp"
            android:textSize="19sp"
            android:textColor="@color/sdk_tv_giftbag_font_hinttitle_color"
            android:text="@string/sdk_huyu_service_center"
            />
        <TextView
            android:id="@+id/sdk_aboutus_worktime_tv"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:gravity="center"
            android:layout_marginTop="11dp"
            android:textSize="16sp"
            android:textColor="@color/sdk_tv_giftbag_font_hinttext_color"
            android:text="@string/sdk_online_service_value"
            />
        <Button
            android:id="@+id/online_service_bt"
            android:layout_width="154dp"
            android:layout_height="43dp"
            android:layout_gravity="center"
            android:layout_marginTop="18dp"
            android:textSize="16sp"
            android:textColor="@color/lepay_background_white"
            android:text="@string/sdk_online_service_name"
            android:background="@drawable/btn_giftbag_seletor"/>

        <LinearLayout
            android:layout_width="match_parent"
            android:layout_height="match_parent"
            android:layout_marginTop="40dp"
            android:background="@drawable/aboutus_bottom_corner"
            android:layout_gravity="center">
            <TextView
                android:id="@+id/le_about_us_sdk_version"
                android:layout_width="wrap_content"
                android:layout_height="match_parent"
                android:gravity="left|center_vertical"
                android:textColor="@color/sdk_tv_giftbag_font_hinttext_color"
                android:layout_marginLeft="23dp"
                android:text="V2.2.5"
                android:textSize="14sp"/>

            <LinearLayout
                android:layout_width="match_parent"
                android:layout_height="match_parent"
                android:gravity="right|center"
                android:layout_marginRight="23dp"
                android:orientation="horizontal">

                <TextView
                    android:id="@+id/hotline_service_name"
                    style="@style/LeStyleBlack14"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:text="@string/sdk_hotline_service_name"
                    android:textSize="14sp"
                    android:textColor="@color/sdk_tv_giftbag_font_hinttext_color"/>

                <TextView
                    android:id="@+id/hotline_service_value"
                    style="@style/LeStyleBlack14"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:text="@string/sdk_hotline_service_value"
                    android:textSize="14sp"
                    android:textColor="@color/sdk_tv_giftbag_font_hinttext_color"/>
                <TextView
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:textSize="14sp"
                    android:textColor="@color/sdk_tv_giftbag_font_hinttext_color"
                    android:text="  è½¬7"/>
            </LinearLayout>

        </LinearLayout>


    </LinearLayout>

    </com.le.legamesdk.widget.LeMainViewParent>

</RelativeLayout>
