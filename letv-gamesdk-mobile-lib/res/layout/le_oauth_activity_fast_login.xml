    <RelativeLayout
        android:id="@+id/le_main_view_parent"
        android:layout_width="320dp"
        android:layout_height="300dp"
        android:layout_centerInParent="true"
        android:background="@drawable/le_oauth_fast_login_main_bg">


        <ImageView
            android:id="@+id/le_oauth_fast_login_logo"
            android:layout_width="80dp"
            android:layout_height="30dp"
            android:layout_centerHorizontal="true"
            android:layout_marginTop="20dp"
            android:src="@drawable/le_oauth_logo_small" />

        <TextView
            android:id="@+id/le_oauth_fast_login_slogan"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:layout_marginTop="75dp"
            android:gravity="center_horizontal"
            android:text="@string/le_oauth_fast_login_single_card_slogan"
            android:textColor="@color/le_oauth_gray" />

        <LinearLayout
            android:id="@+id/le_oauth_fast_login_double_card_parent"
            android:layout_width="match_parent"
            android:layout_height="match_parent"
            android:layout_below="@id/le_oauth_fast_login_slogan"
            android:layout_marginTop="10dp"
            android:gravity="center_horizontal"
            android:orientation="vertical"
            android:visibility="gone">

            <RelativeLayout
                android:id="@+id/le_oauth_fast_login_phoneNumber1_parent"
                android:layout_width="200dp"
                android:layout_height="36dp">

                <View
                    android:id="@+id/le_oauth_fast_login_phoneNumber1_icon"
                    android:layout_width="20dp"
                    android:layout_height="20dp"
                    android:layout_alignParentLeft="true"
                    android:layout_alignParentStart="true"
                    android:layout_centerVertical="true"
                    android:background="@drawable/le_oauth_check_box_bg" />

                <TextView
                    android:id="@+id/le_oauth_fast_login_phoneNumber1"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_centerVertical="true"
                    android:layout_marginLeft="10dp"
                    android:layout_marginStart="10dp"
                    android:layout_toEndOf="@id/le_oauth_fast_login_phoneNumber1_icon"
                    android:layout_toRightOf="@id/le_oauth_fast_login_phoneNumber1_icon"
                    android:maxLines="1"
                    android:textColor="@color/le_oauth_fast_login_text_color"
                    android:textSize="18dp" />

            </RelativeLayout>

            <View
                android:layout_width="200dp"
                android:layout_height="1dp"
                android:background="@drawable/le_oauth_gap_line_bg" />

            <RelativeLayout
                android:id="@+id/le_oauth_fast_login_phoneNumber2_parent"
                android:layout_width="200dp"
                android:layout_height="36dp">

                <View
                    android:id="@+id/le_oauth_fast_login_phoneNumber2_icon"
                    android:layout_width="20dp"
                    android:layout_height="20dp"
                    android:layout_alignParentLeft="true"
                    android:layout_alignParentStart="true"
                    android:layout_centerVertical="true"
                    android:background="@drawable/le_oauth_check_box_bg" />

                <TextView
                    android:id="@+id/le_oauth_fast_login_phoneNumber2"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_centerVertical="true"
                    android:layout_marginLeft="10dp"
                    android:layout_marginStart="10dp"
                    android:layout_toEndOf="@id/le_oauth_fast_login_phoneNumber2_icon"
                    android:layout_toRightOf="@id/le_oauth_fast_login_phoneNumber2_icon"
                    android:maxLines="1"
                    android:textColor="@color/le_oauth_fast_login_text_color"
                    android:textSize="18dp" />

            </RelativeLayout>

        </LinearLayout>

        <LinearLayout
            android:id="@+id/le_oauth_fast_login_single_card_parent"
            android:layout_width="match_parent"
            android:layout_height="match_parent"
            android:layout_below="@id/le_oauth_fast_login_slogan"
            android:layout_marginTop="20dp"
            android:gravity="center_horizontal"
            android:orientation="vertical"
            android:visibility="visible">

            <RelativeLayout
                android:id="@+id/le_oauth_fast_login_phoneNumber_parent"
                android:layout_width="wrap_content"
                android:layout_height="36dp">

                <View
                    android:id="@+id/le_oauth_fast_login_phoneNumber_icon"
                    android:layout_width="20dp"
                    android:layout_height="20dp"
                    android:layout_alignParentLeft="true"
                    android:layout_alignParentStart="true"
                    android:layout_centerVertical="true"
                    android:background="@drawable/le_oauth_check_box_bg_true" />

                <TextView
                    android:id="@+id/le_oauth_fast_login_phoneNumber"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_centerVertical="true"
                    android:layout_marginLeft="10dp"
                    android:layout_marginStart="10dp"
                    android:layout_toEndOf="@id/le_oauth_fast_login_phoneNumber_icon"
                    android:layout_toRightOf="@id/le_oauth_fast_login_phoneNumber_icon"
                    android:maxLines="1"
                    android:textColor="@color/le_oauth_orange"
                    android:textSize="18dp" />

            </RelativeLayout>
        </LinearLayout>

        <TextView
            android:id="@+id/le_fast_login_user_other_account_login"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_alignParentBottom="true"
            android:layout_alignParentEnd="true"
            android:layout_alignParentRight="true"
            android:layout_centerVertical="true"
            android:layout_marginBottom="10dp"
            android:layout_marginEnd="20dp"
            android:layout_marginRight="20dp"
            android:text="@string/le_oauth_account_manager_other_login"
            android:textColor="@color/le_oauth_orange"
            android:textSize="14dp" />

        <LinearLayout
            android:id="@+id/le_fast_login_user_terms_parent"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:layout_above="@id/le_fast_login_user_other_account_login"
            android:layout_marginBottom="10dp"
            android:layout_marginEnd="20dp"
            android:layout_marginLeft="20dp"
            android:layout_marginRight="20dp"
            android:layout_marginStart="20dp"
            android:gravity="center_vertical"
            android:orientation="horizontal">

            <TextView
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:text="@string/le_oauth_fast_login_agree"
                android:textColor="@color/le_oauth_gray"
                android:textSize="12dp" />

            <TextView
                android:id="@+id/le_fast_login_user_terms"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:autoLink="web"
                android:text="@string/le_oauth_fast_login_user_terms"
                android:textColor="#555555"
                android:textSize="12dp" />

            <TextView
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:text="@string/le_oauth_fast_login_and"
                android:textColor="@color/le_oauth_gray"
                android:textSize="12dp" />

            <TextView
                android:id="@+id/le_fast_login_privacy_agreement"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:autoLink="web"
                android:text="@string/le_oauth_fast_login_privacy_agreement"
                android:textColor="#555555"
                android:textSize="12dp" />
        </LinearLayout>


        <Button
            android:id="@+id/le_fast_login_now_btn"
            android:layout_width="280dp"
            android:layout_height="40dp"
            android:layout_above="@id/le_fast_login_user_terms_parent"
            android:layout_centerHorizontal="true"
            android:layout_marginBottom="10dp"
            android:background="@drawable/le_oauth_fast_login_button_background"
            android:text="@string/le_oauth_fast_login_login_immediately"
            android:textColor="@color/le_oauth_text_color_white"
            android:textSize="16dp" />


    </RelativeLayout>

    <View
        android:id="@+id/le_fast_login_cover_layout"
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        android:visibility="gone" />

</RelativeLayout>
