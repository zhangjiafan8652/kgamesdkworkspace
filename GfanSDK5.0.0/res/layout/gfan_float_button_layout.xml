<?xml version="1.0" encoding="utf-8"?>
<merge xmlns:android="http://schemas.android.com/apk/res/android">

    <ImageView
        android:id="@+id/gfan_float_view_icon"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_gravity="left"
        android:scaleType="centerCrop"
        android:src="@drawable/gfan_floatview_icon" />

    <ImageView
        android:id="@+id/gfan_float_msg_icon_right"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_gravity="right"
        android:src="@drawable/gfan_float_icon_msg_right"
        android:visibility="gone" />

    <ImageView
        android:id="@+id/gfan_float_msg_icon_left"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:src="@drawable/gfan_float_icon_msg_left"
        android:layout_gravity="left"
        android:visibility="gone" />

</merge>