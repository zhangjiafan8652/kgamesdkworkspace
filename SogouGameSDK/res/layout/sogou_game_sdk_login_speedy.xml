<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:id="@+id/sogou_game_sdk_exit_dialog"
    android:layout_width="267dp"
    android:layout_height="153dp"
    android:background="@drawable/sogou_game_sdk_exit_dialog_bg"
    android:orientation="horizontal" >

    <RelativeLayout
        android:id="@+id/sogou_game_sdk_login_speedy_above"
        android:layout_width="fill_parent"
        android:layout_height="120dp" >

        <View
            android:id="@+id/sogou_game_sdk_login_speedy_strut"
            android:layout_width="0dp"
            android:layout_height="0dp"
            android:layout_centerHorizontal="true" />

        <RelativeLayout
            android:id="@+id/relativeLayout1"
            android:layout_width="0dp"
            android:layout_height="wrap_content"
            android:layout_alignParentLeft="true"
            android:layout_alignRight="@id/sogou_game_sdk_login_speedy_strut"
            android:layout_marginTop="24dp" >

            <TextView
                android:id="@+id/sogou_game_sdk_login_qq_tv"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_centerHorizontal="true"
                android:text="@string/sogou_game_sdk_login_qq_sso"
                android:textColor="@color/sogou_game_sdk_color_000000"
                android:textSize="14sp"
                android:textStyle="bold" />

            <Button
                android:id="@+id/sogou_game_sdk_login_qq_btn"
                android:layout_width="80dp"
                android:layout_height="55dp"
                android:layout_below="@id/sogou_game_sdk_login_qq_tv"
                android:layout_centerHorizontal="true"
                android:layout_marginTop="13dp"
                android:background="@drawable/sogou_game_sdk_login_qq" />
        </RelativeLayout>

        <RelativeLayout
            android:id="@+id/relativeLayout2"
            android:layout_width="0dp"
            android:layout_height="wrap_content"
            android:layout_alignLeft="@id/sogou_game_sdk_login_speedy_strut"
            android:layout_alignParentRight="true"
            android:layout_marginTop="24dp"
          >

            <TextView
                android:id="@+id/sogou_game_sdk_login_express_tv"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:text="@string/sogou_game_sdk_login_express"
                android:textColor="@color/sogou_game_sdk_color_000000"
                android:textSize="14sp"
                android:layout_centerHorizontal="true"
                android:textStyle="bold" />

            <TextView
                android:id="@+id/sogou_game_sdk_login_express_user_tv"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_below="@id/sogou_game_sdk_login_express_tv"
                android:layout_centerHorizontal="true"
                android:layout_marginTop="10dp"
                android:text="@string/sogou_game_sdk_login_express_user"
                android:textColor="@color/sogou_game_sdk_color_7d7d7d"
                android:textSize="12sp" />

            <Button
                android:id="@+id/sogou_game_sdk_login_express_btn"
                android:layout_width="80dp"
                android:layout_height="33.5dp"
                android:layout_below="@id/sogou_game_sdk_login_express_user_tv"
                android:layout_centerHorizontal="true"
                android:layout_marginLeft="24dp"
                android:layout_marginTop="10dp"
                android:background="@drawable/sogou_game_sdk_bt_red"
                android:text="@string/sogou_game_sdk_login_akey"
                android:textColor="@color/sogou_game_sdk_color_ffffff"
                android:textSize="13sp" />
        </RelativeLayout>

        <View
            android:id="@+id/view1"
            android:layout_width="1dp"
            android:layout_height="90dp"
            android:layout_alignTop="@+id/relativeLayout1"
            android:layout_centerHorizontal="true"
            android:background="@color/sogou_game_sdk_color_gray" />

    </RelativeLayout>

    <RelativeLayout
        android:layout_width="match_parent"
        android:layout_height="33dp"
        android:layout_below="@id/sogou_game_sdk_login_speedy_above"
        android:background="@drawable/sogou_game_sdk_login_speedy_bottom_bg"
        android:gravity="center" >

        <TextView
            android:id="@+id/sogou_game_sdk_login_other_tv"
            android:layout_width="wrap_content"
            android:layout_height="fill_parent"
            android:gravity="center"
            android:text="@string/sogou_game_sdk_login_other"
            android:textColor="@color/sogou_game_sdk_color_7d7d7d"
            android:textSize="13sp" />
    </RelativeLayout>

</RelativeLayout>