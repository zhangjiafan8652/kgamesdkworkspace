<?xml version="1.0" encoding="utf-8"?>
<resources>

    <declare-styleable name="TextWithLine"></declare-styleable>
    <declare-styleable name="bdp_ViewPagerIndicator">
        <attr name="bdp_vpiIconPageIndicatorStyle" format="reference" />
        <attr name="bdp_vpiTabPageIndicatorStyle" format="reference" />
    </declare-styleable>
    <declare-styleable name="CircleProgressBar">
        <attr name="bdp_inner_radius" format="dimension" />
        <attr name="bdp_background_color" format="color" />
        <attr name="bdp_progress_color" format="color" />
        <attr name="bdp_progress_stoke_width" format="dimension" />
        <attr name="bdp_show_arrow" format="boolean" />
        <attr name="bdp_enable_circle_background" format="boolean" />
        <attr name="bdp_arrow_width" format="dimension" />
        <attr name="bdp_arrow_height" format="dimension" />
        <attr name="bdp_progress" format="integer" />
        <attr name="bdp_max" format="integer" />
        <attr name="bdp_progress_text_size" format="dimension" />
        <attr name="bdp_progress_text_color" format="color" />
        <attr name="bdp_progress_text_visibility" format="enum">
            <enum name="visible" value="0" />
            <enum name="invisible" value="1" />
        </attr>
    </declare-styleable>

</resources>