<?xml version="1.0" encoding="utf-8"?>
<layer-list xmlns:android="http://schemas.android.com/apk/res/android">

    <item android:id="@android:id/background">
        <shape>
            <corners android:radius="5dp" />
            <gradient
                android:startColor="#A5804E"
                android:centerColor="#A5804E"
                android:centerY="0.75"
                android:endColor="#A5804E"
                android:angle="270"
                />

        </shape>
    </item>

    <item android:id="@android:id/secondaryProgress">
        <clip>
            <shape>
                <corners android:radius="5dp" />
                <gradient
                    android:startColor="#A5804E"
                    android:centerColor="#A5804E"
                    android:centerY="0.75"
                    android:endColor="#A5804E"
                    android:angle="270"
                    />
            </shape>
        </clip>
    </item>

    <item android:id="@android:id/progress">
        <clip>
            <shape>
                <corners android:radius="5dp" />
                <gradient
                    android:startColor="#FF6C00"
                    android:centerColor="#FF6C00"
                    android:centerY="0.75"
                    android:endColor="#FF6C00"
                    android:angle="270"
                    />

            </shape>
        </clip>
    </item>

</layer-list>