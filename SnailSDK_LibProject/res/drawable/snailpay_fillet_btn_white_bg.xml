<?xml version="1.0" encoding="utf-8"?>
<selector xmlns:android="http://schemas.android.com/apk/res/android">
    <item android:state_enabled="false">
        <shape>
            <solid android:color="#979797" />
            <corners android:radius="@dimen/snailpay_btn_corners" />
        </shape>
        </item>
    <item android:state_pressed="true" >
        <shape>
            <solid android:color="@color/snailpay_btn_white_click" />
            <corners android:radius="@dimen/snailpay_btn_corners" />
            <stroke android:width="1dp" android:color="#dcdcdc" />
        </shape>
        </item>
    <item>
        <shape>
            <solid android:color="@color/snailpay_btn_white_normal" />
            <stroke android:width="1dp" android:color="#dcdcdc" />
            <corners android:radius="@dimen/snailpay_btn_corners" />
        </shape>
    </item>
</selector>