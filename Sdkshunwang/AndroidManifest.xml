            android:label="app_name" >
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
      
        
      <!-- 手游互联配置 -->
     	 <meta-data
            android:name="app_id"
            android:value="kk2017779022" />
        <meta-data
            android:name="isLandscape"
            android:value="true" />
        
        <meta-data
            android:name="gameid"
            android:value="string10614" />
        <meta-data
            android:name="siteId"
            android:value="string10614" />
        <meta-data
            android:name="md5Key"
            android:value="8D31B4DA5EA248E58D669B4EBC292C77" />
        <meta-data
            android:name="rsaKey"
            android:value="" />
        
        <meta-data
            android:name="callback"
            android:value="https://api.sdk.75757.com/pay/notify/2017779022/" />
        
        <activity
